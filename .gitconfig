[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
	st = status
	qamend = commit --amend --no-edit
[http]
	postBuffer = 524288001
[remote "origin"]
	receivepack = git receive-pack
